#!/usr/bin/env node --napi-modules

const mq = require("../dist/mq.js");
const program = require("commander");

program
  .version("1.2.0")
  .description("message broker")
  .parse(process.argv);

console.log("hello");
mq.broker.startBroker()
  .then(() => process.exit(0))
  .catch((err) => {
    console.error(err);
    process.exit(1);
  });
